<!DOCTYPE html>
<html>
  <head>
    <title>Imobiliaria | Projeto de teste</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>


  <body>
    <div id="wrapper">
      <%= render partial: 'layouts/header' %>
      <%= yield %>
      <%= render partial: 'layouts/footer' %>
    </div>
  </body>
  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'scripts/jquery-3.4.1.min.js' %>
  <%= javascript_include_tag 'scripts/jquery-migrate-3.1.0.min.js' %>
  <%= javascript_include_tag 'scripts/chosen.min.js' %>
  <%= javascript_include_tag 'scripts/magnific-popup.min.js' %>
  <%= javascript_include_tag 'scripts/owl.carousel.min.js' %>
  <%= javascript_include_tag 'scripts/rangeSlider.js' %>
  <%= javascript_include_tag 'scripts/sticky-kit.min.js' %>
  <%= javascript_include_tag 'scripts/slick.min.js' %>
  <%= javascript_include_tag 'scripts/masonry.min.js' %>
  <%= javascript_include_tag 'scripts/mmenu.min.js' %>
  <%= javascript_include_tag 'scripts/tooltips.min.js' %>
  <%= javascript_include_tag 'scripts/custom.js' %>

  <!-- Google Autocomplete -->
  <script>
    function initAutocomplete() {
      var input = document.getElementById('autocomplete-input');
      var autocomplete = new google.maps.places.Autocomplete(input);

      autocomplete.addListener('place_changed', function() {
        var place = autocomplete.getPlace();
        if (!place.geometry) {
          window.alert("No details available for input: '" + place.name + "'");
          return;
        }
      });
  }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&callback=initAutocomplete"></script>
</html>
